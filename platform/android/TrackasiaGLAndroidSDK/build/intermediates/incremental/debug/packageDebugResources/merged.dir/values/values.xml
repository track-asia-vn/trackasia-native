        <attr format="dimension" name="maplibre_uiCompassMarginTop"/>
        <attr format="dimension" name="maplibre_uiCompassMarginRight"/>
        <attr format="dimension" name="maplibre_uiCompassMarginBottom"/>
        <attr format="boolean" name="maplibre_uiCompassFadeFacingNorth"/>
        <attr format="reference" name="maplibre_uiCompassDrawable"/>

        
        <attr format="boolean" name="maplibre_uiLogo"/>
        <attr name="maplibre_uiLogoGravity">
            <flag name="top" value="0x30"/>
            <flag name="bottom" value="0x50"/>
            <flag name="left" value="0x03"/>
            <flag name="right" value="0x05"/>
            <flag name="center_vertical" value="0x10"/>
            <flag name="fill_vertical" value="0x70"/>
            <flag name="center_horizontal" value="0x01"/>
            <flag name="fill_horizontal" value="0x07"/>
            <flag name="center" value="0x11"/>
            <flag name="fill" value="0x77"/>
            <flag name="clip_vertical" value="0x80"/>
            <flag name="clip_horizontal" value="0x08"/>
            <flag name="start" value="0x00800003"/>
            <flag name="end" value="0x00800005"/>
        </attr>
        <attr format="dimension" name="maplibre_uiLogoMarginLeft"/>
        <attr format="dimension" name="maplibre_uiLogoMarginTop"/>
        <attr format="dimension" name="maplibre_uiLogoMarginRight"/>
        <attr format="dimension" name="maplibre_uiLogoMarginBottom"/>

        
        <attr format="boolean" name="maplibre_uiAttribution"/>
        <attr name="maplibre_uiAttributionGravity">
            <flag name="top" value="0x30"/>
            <flag name="bottom" value="0x50"/>
            <flag name="left" value="0x03"/>
            <flag name="right" value="0x05"/>
            <flag name="center_vertical" value="0x10"/>
            <flag name="fill_vertical" value="0x70"/>
            <flag name="center_horizontal" value="0x01"/>
            <flag name="fill_horizontal" value="0x07"/>
            <flag name="center" value="0x11"/>
            <flag name="fill" value="0x77"/>
            <flag name="clip_vertical" value="0x80"/>
            <flag name="clip_horizontal" value="0x08"/>
            <flag name="start" value="0x00800003"/>
            <flag name="end" value="0x00800005"/>
        </attr>
        <attr format="dimension" name="maplibre_uiAttributionMarginLeft"/>
        <attr format="dimension" name="maplibre_uiAttributionMarginTop"/>
        <attr format="dimension" name="maplibre_uiAttributionMarginRight"/>
        <attr format="dimension" name="maplibre_uiAttributionMarginBottom"/>
        <attr format="color" name="maplibre_uiAttributionTintColor"/>

        
        <attr format="boolean" name="maplibre_renderTextureMode"/>
        <attr format="boolean" name="maplibre_renderTextureTranslucentSurface"/>
        <attr format="color" name="maplibre_foregroundLoadColor"/>

        <attr format="boolean" name="maplibre_enableTilePrefetch"/>
        <attr format="integer" name="maplibre_prefetchZoomDelta"/>
        <attr format="boolean" name="maplibre_enableZMediaOverlay"/>
        <attr format="float" name="maplibre_pixelRatio"/>

