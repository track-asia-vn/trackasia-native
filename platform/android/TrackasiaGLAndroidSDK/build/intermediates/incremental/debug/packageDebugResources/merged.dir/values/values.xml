<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="maplibre_blue">#1E8CAB</color>
    <color name="maplibre_gray">#7D7F80</color>
    <color name="maplibre_gray_dark">#5F5F5F</color>
    <color name="maplibre_location_layer_blue">#4A90E2</color>
    <color name="maplibre_location_layer_gray">#A1B0C0</color>
    <dimen name="maplibre_angular_velocity_multiplier">1.3dp</dimen>
    <dimen name="maplibre_density_constant">0.29dp</dimen>
    <dimen name="maplibre_eight_dp">8dp</dimen>
    <dimen name="maplibre_four_dp">4dp</dimen>
    <dimen name="maplibre_infowindow_margin">8dp</dimen>
    <dimen name="maplibre_infowindow_tipview_width">8dp</dimen>
    <dimen name="maplibre_locationComponentTrackingInitialMoveThreshold">25dp</dimen>
    <dimen name="maplibre_locationComponentTrackingMultiFingerMoveThreshold">400dp</dimen>
    <dimen name="maplibre_minimum_angled_scale_speed">0.9dp</dimen>
    <dimen name="maplibre_minimum_angular_velocity">0.10dp</dimen>
    <dimen name="maplibre_minimum_scale_span_when_rotating">75dp</dimen>
    <dimen name="maplibre_minimum_scale_speed">0.6dp</dimen>
    <dimen name="maplibre_minimum_scale_velocity">225dp</dimen>
    <dimen name="maplibre_my_locationview_outer_circle">18dp</dimen>
    <dimen name="maplibre_ninety_two_dp">92dp</dimen>
    <string name="maplibre_attributionErrorNoBrowser">No web browser installed on device, can\'t open web page.</string>
    <string name="maplibre_attributionsDialogTitle">MapLibre Maps SDK for Android</string>
    <string name="maplibre_attributionsIconContentDescription">Attribution icon. Activate to show attribution dialog.</string>
    <string name="maplibre_compassContentDescription">Map compass. Activate to reset the map rotation to North.</string>
    <string name="maplibre_mapActionDescription">Showing a Map created with MapLibre. Scroll by dragging two fingers. Zoom by pinching two fingers.</string>
    <string name="maplibre_myLocationViewContentDescription">Location View. This shows your location on the map.</string>
    <string name="maplibre_offline_error_region_definition_invalid">Provided OfflineRegionDefinition doesn\'t fit the world bounds: %s</string>
    <style name="maplibre_LocationComponent">
    <item name="maplibre_foregroundDrawable">@drawable/maplibre_user_icon</item>
    <item name="maplibre_backgroundDrawable">@drawable/maplibre_user_stroke_icon</item>
    <item name="maplibre_bearingDrawable">@drawable/maplibre_user_bearing_icon</item>
    <item name="maplibre_gpsDrawable">@drawable/maplibre_user_puck_icon</item>

    <item name="maplibre_foregroundDrawableStale">@drawable/maplibre_user_icon_stale</item>
    <item name="maplibre_backgroundDrawableStale">@drawable/maplibre_user_stroke_icon</item>

    <item name="maplibre_accuracyAlpha">0.15</item>
    <item name="maplibre_accuracyColor">@color/maplibre_location_layer_blue</item>

    <item name="maplibre_elevation">12dp</item>

    <item name="maplibre_enableStaleState">true</item>
    <item name="maplibre_staleStateTimeout">30000</item>

    
    <item name="maplibre_iconPaddingLeft">0</item>
    <item name="maplibre_iconPaddingTop">0</item>
    <item name="maplibre_iconPaddingRight">0</item>
    <item name="maplibre_iconPaddingBottom">0</item>

    
    <item name="maplibre_maxZoomIconScale">1</item>
    <item name="maplibre_minZoomIconScale">0.6</item>

    
    <item name="maplibre_trackingGesturesManagement">false</item>
    <item name="maplibre_trackingInitialMoveThreshold">@dimen/maplibre_locationComponentTrackingInitialMoveThreshold</item>
    <item name="maplibre_trackingMultiFingerMoveThreshold">@dimen/maplibre_locationComponentTrackingMultiFingerMoveThreshold</item>

    
    <item name="maplibre_pulsingLocationCircleEnabled">false</item>
    <item name="maplibre_pulsingLocationCircleFadeEnabled">true</item>
    <item name="maplibre_pulsingLocationCircleColor">@color/maplibre_location_layer_blue</item>
    <item name="maplibre_pulsingLocationCircleDuration">2300</item>
    <item name="maplibre_pulsingLocationCircleRadius">35</item>
    <item name="maplibre_pulsingLocationCircleAlpha">0.4</item>
    <item name="maplibre_pulsingLocationCircleInterpolator">decelerate</item>

  </style>
    <declare-styleable name="maplibre_BubbleLayout">
        <attr format="dimension|reference" name="maplibre_bl_arrowWidth"/>
        <attr format="dimension|reference" name="maplibre_bl_cornersRadius"/>
        <attr format="dimension|reference" name="maplibre_bl_arrowHeight"/>
        <attr format="dimension|reference" name="maplibre_bl_arrowPosition"/>
        <attr format="color|reference" name="maplibre_bl_bubbleColor"/>
        <attr format="dimension|reference" name="maplibre_bl_strokeWidth"/>
        <attr format="color|reference" name="maplibre_bl_strokeColor"/>

        <attr format="enum" name="maplibre_bl_arrowDirection">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
            <enum name="top" value="2"/>
            <enum name="bottom" value="3"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="maplibre_LocationComponent">
        <attr format="reference" name="maplibre_foregroundDrawable"/>
        <attr format="color" name="maplibre_foregroundTintColor"/>
        <attr format="reference" name="maplibre_backgroundDrawable"/>
        <attr format="color" name="maplibre_backgroundTintColor"/>
        <attr format="reference" name="maplibre_bearingDrawable"/>
        <attr format="color" name="maplibre_bearingTintColor"/>
        <attr format="reference" name="maplibre_gpsDrawable"/>

        <attr format="reference" name="maplibre_foregroundDrawableStale"/>
        <attr format="color" name="maplibre_foregroundStaleTintColor"/>
        <attr format="reference" name="maplibre_backgroundDrawableStale"/>
        <attr format="color" name="maplibre_backgroundStaleTintColor"/>

        <attr format="float" name="maplibre_accuracyAlpha"/>
        <attr format="color" name="maplibre_accuracyColor"/>

        <attr format="dimension" name="maplibre_elevation"/>

        
        <attr format="boolean" name="maplibre_enableStaleState"/>
        <attr format="integer" min="0" name="maplibre_staleStateTimeout"/>

        
        <attr format="integer" name="maplibre_iconPaddingLeft"/>
        <attr format="integer" name="maplibre_iconPaddingTop"/>
        <attr format="integer" name="maplibre_iconPaddingRight"/>
        <attr format="integer" name="maplibre_iconPaddingBottom"/>

        
        <attr format="string" name="maplibre_layer_above"/>
        <attr format="string" name="maplibre_layer_below"/>

        
        <attr format="float" name="maplibre_maxZoomIconScale"/>
        <attr format="float" name="maplibre_minZoomIconScale"/>

        
        <attr format="boolean" name="maplibre_trackingGesturesManagement"/>
        <attr format="dimension" name="maplibre_trackingInitialMoveThreshold"/>
        <attr format="dimension" name="maplibre_trackingMultiFingerMoveThreshold"/>

        
        <attr format="float" name="maplibre_trackingAnimationDurationMultiplier"/>

        
        <attr format="boolean" name="maplibre_compassAnimationEnabled"/>

        
        <attr format="boolean" name="maplibre_accuracyAnimationEnabled"/>

        
        <attr format="boolean" name="maplibre_pulsingLocationCircleEnabled"/>
        <attr format="boolean" name="maplibre_pulsingLocationCircleFadeEnabled"/>
        <attr format="color" name="maplibre_pulsingLocationCircleColor"/>
        <attr format="float" name="maplibre_pulsingLocationCircleDuration"/>
        <attr format="float" name="maplibre_pulsingLocationCircleRadius"/>
        <attr format="float" name="maplibre_pulsingLocationCircleAlpha"/>
        <attr format="string" name="maplibre_pulsingLocationCircleInterpolator"/>

    </declare-styleable>
    <declare-styleable name="maplibre_MapView">

        
        <attr format="string" name="maplibre_apiBaseUrl"/>

        
        <attr format="string" name="maplibre_apiBaseUri"/>
        <attr format="boolean" name="maplibre_localIdeographEnabled"/>
        <attr format="string" name="maplibre_localIdeographFontFamily"/>
        <attr format="reference" name="maplibre_localIdeographFontFamilies"/>
        <attr format="boolean" name="maplibre_cross_source_collisions"/>

        
        <attr format="float" name="maplibre_cameraTargetLat"/>
        <attr format="float" name="maplibre_cameraTargetLng"/>
        <attr format="float" name="maplibre_cameraZoom"/>
        <attr format="float" name="maplibre_cameraBearing"/>
        <attr format="float" name="maplibre_cameraTilt"/>

        
        <attr format="float" name="maplibre_cameraZoomMax"/>
        <attr format="float" name="maplibre_cameraZoomMin"/>

        
        <attr format="float" name="maplibre_cameraPitchMax"/>
        <attr format="float" name="maplibre_cameraPitchMin"/>

        
        <attr format="boolean" name="maplibre_uiZoomGestures"/>
        <attr format="boolean" name="maplibre_uiScrollGestures"/>
        <attr format="boolean" name="maplibre_uiHorizontalScrollGestures"/>
        <attr format="boolean" name="maplibre_uiRotateGestures"/>
        <attr format="boolean" name="maplibre_uiTiltGestures"/>
        <attr format="boolean" name="maplibre_uiDoubleTapGestures"/>
        <attr format="boolean" name="maplibre_uiQuickZoomGestures"/>

        
        <attr format="boolean" name="maplibre_uiCompass"/>
